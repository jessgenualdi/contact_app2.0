<h1>Here are your contacts</h1>


<% @contacts.each do |contact| %>
  <h2> <%= contact.full_name   %></h2>
  <p><%= contact.bio%></p>
  <p><%= contact.email %></p>
  <p><%= contact.japanese_prefix %></p>
  <p><%= contact.pretty_created_at %></p>

  <a href="/contacts/<%= contact.id%>">Show Contact</a>
<% end %>